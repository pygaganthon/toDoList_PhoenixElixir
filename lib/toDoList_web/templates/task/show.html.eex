<h1>Show Task</h1>

<ul>

  <li>
    <strong>Input:</strong>
    <%= @task.input %>
  </li>

  <li>
    <strong>User:</strong>
    <%= @task.user_id %>
  </li>

  <li>
    <strong>Status:</strong>
    <%= @task.status %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.task_path(@conn, :edit, @task) %></span>
<span><%= link "Back", to: Routes.task_path(@conn, :index) %></span>
